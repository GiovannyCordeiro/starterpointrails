<h2>Vehicles</h2>

<p>
  <%= link_to "New Vehicle", new_vehicle_path %>
</p>

<% @vehicles.each do |vehicle| %>
  <p>
    <%= vehicle.id %> |
    <%= vehicle.brand %> |
    <%= vehicle.model %> |
    <%= vehicle.year %> |
    <%= vehicle.kind %> |
    <%= link_to 'Details', vehicle_path(vehicle) %>
  </p>
<% end %>