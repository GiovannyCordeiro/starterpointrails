<h2>Vehicles</h2>

<p>
  <%= link_to "New Vehicle", new_vehicle_path %>
</p>

<% @vehicles.each do |vehicle| %>
  <p>
    <%= link_to vehicle_path(vehicle) do %>
      <%= vehicle.id %> |
    <% end %>
    <%= vehicle.brand %> |
    <%= vehicle.model %> |
    <%= vehicle.year %> |
    <%= vehicle.kind %> |
  </p>
<% end %>